CREATE DATABASE pregsource;

\connect pregsource

CREATE TABLE resources(
srcnum serial,
srctopic varchar(255),
srcurl varchar(2083) NOT NULL,
firsttri varchar(3),
secondtri varchar (3),
thirdtri varchar(3)
);

ALTER TABLE resources
ADD COLUMN postpartum varchar(3);

INSERT INTO resources(srctopic, srcurl, firsttri, secondtri, thirdtri, postpartum)
